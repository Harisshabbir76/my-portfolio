{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "client/dist",
  "rewrites": [
    { "source": "/api", "destination": "/api/index" },
    { "source": "/api/contact", "destination": "/api/contact" },
    { "source": "/api/health", "destination": "/api/health" },
    { "source": "/(.*)", "destination": "/" }
  ]
}